<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BifacialView">
        <attr name="drawableLeft" format="reference" />
        <attr name="drawableRight" format="reference" />
        <attr name="delimiterColor" format="color" />
        <attr name="arrowColor" format="color" />
        <attr name="arrowVisibility" format="boolean" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="leftText" format="string" />
        <attr name="rightText" format="string" />
        <attr name="touchMode" format="enum">
            <enum name="all" value="0" />
            <enum name="delimiter" value="1" />
        </attr>

    </declare-styleable>
</resources>